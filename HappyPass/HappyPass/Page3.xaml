<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HappyPass.Page3" Icon="locationsmallicon.png"
             BackgroundImage="map.png">>
    <ContentPage.Content>
        <StackLayout>
            <ImageButton Source="mapicon.png" x:Name="offer1"  VerticalOptions="Center" HorizontalOptions="Center" Margin="35, 5, 0, 0" HeightRequest="50" WidthRequest="50"/>
            <ImageButton Source="mapicon.png" x:Name="offer2"  VerticalOptions="End" HorizontalOptions="Start" Margin="35, 5, 0, 0" HeightRequest="50" WidthRequest="50"/>
            <ImageButton Source="mapicon.png" x:Name="offer3"  VerticalOptions="Start" HorizontalOptions="Center" Margin="35, 5, 0, 0" HeightRequest="50" WidthRequest="50"/>
            <ImageButton Source="mapicon.png" x:Name="offer4"  VerticalOptions="Center" HorizontalOptions="End" Margin="35,15, 0, 55" HeightRequest="50" WidthRequest="50"/>
            <ImageButton Source="mapicon.png" x:Name="offer5"  VerticalOptions="Start" HorizontalOptions="Center" Margin="35, 5, 0, 0" HeightRequest="50" WidthRequest="50"/>
            <ImageButton Source="mapicon.png" x:Name="offer6"  VerticalOptions="End" HorizontalOptions="End" Margin="10, 0, 10, 5" HeightRequest="50" WidthRequest="50"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>